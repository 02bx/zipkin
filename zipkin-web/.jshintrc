{
  // Environment
  "browser"   : true,
  "jquery"    : true,

  // Relaxing options
  "laxcomma"  : true,        // Suppress warnings about comma style

  // Enforcing options
  "bitwise"   : true,        // Disable bitwise operators
  "curly"     : true,        // Require curly braces around blocks
  "plusplus"  : false,       // Prohibits unary increment (++) and decrement (--) operators
  "undef"     : true,        // Prohibits use of undeclared variables

  // Misc
  "maxerr"    : 100         // Max errors before stopping
}

